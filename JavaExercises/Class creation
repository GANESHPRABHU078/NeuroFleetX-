
class Student {
    String name;
    int age;
}

public class ClassExample1 {
    public static void main(String[] args) {
        // Create object of Student
        Student s1 = new Student();
        s1.name = "Ganesh";
        s1.age = 20;

        System.out.println("Name: " + s1.name);
        System.out.println("Age: " + s1.age);
    }
}
