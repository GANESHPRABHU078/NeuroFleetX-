class Student {
    String name;
    int age;
    Student(String n, int a) {
        name = n;
        age = a;
    }

    void display() {
        System.out.println("Name: " + name);
        System.out.println("Age: " + age);
    }
}

public class ClassExample3 {
    public static void main(String[] args) {
        Student s1 = new Student("Ganesh", 20);
        s1.display();
    }
}
